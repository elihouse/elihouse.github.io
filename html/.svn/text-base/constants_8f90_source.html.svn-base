<a name="l00124"></a>00124     <span class="keywordtype">integer</span>, <span class="keywordtype">intent(in)</span> :: solver
<a name="l00125"></a>00125 
<a name="l00126"></a>00126     <span class="keyword">if</span> (solver.eq.<a class="code" href="namespaceconstants.html#a87f576192720d53dfa02adbb8485a59a">SOLVER_LAX</a>) <span class="keyword">then</span>
