<a name="l00103"></a>00103   <span class="keyword">character(10) </span><span class="keyword">function </span>bcname (bc_type)
<a name="l00104"></a>00104 
<a name="l00105"></a>00105     <span class="keywordtype">integer</span>, <span class="keywordtype">intent(in)</span> :: bc_type
<a name="l00106"></a>00106 
<a name="l00107"></a>00107     <span class="keyword">if</span> (bc_type.eq.<a class="code" href="namespaceconstants.html#ae4427e4e560515da1809606c7401f2f1">BC_REFLECTIVE</a>) <span class="keyword">then</span>
<a name="l00108"></a>00108       bcname = <span class="stringliteral">&#39;Reflective&#39;</span>
<a name="l00109"></a>00109     <span class="keyword">else</span> <span class="keyword">if</span> (bc_type.eq.<a class="code" href="namespaceconstants.html#af75d78c3ed566b7ba8e80d21f44adfff">BC_FREE_FLOW</a>) <span class="keyword">then</span>
<a name="l00110"></a>00110       bcname = <span class="stringliteral">&#39;Free flow&#39;</span>
<a name="l00111"></a>00111     <span class="keyword">else</span> <span class="keyword">if</span> (bc_type.eq.<a class="code" href="namespaceconstants.html#ae908176373d4e9ea0eafd875c1cce510">BC_PERIODIC</a>) <span class="keyword">then</span>
<a name="l00112"></a>00112       bcname = <span class="stringliteral">&#39;Periodic&#39;</span>
<a name="l00113"></a>00113     <span class="keyword">else</span> <span class="keyword">if</span> (bc_type.eq.<a class="code" href="namespaceconstants.html#ad7d69b66d2d9074e1b125d5222cf7427">BC_SPECIAL</a>) <span class="keyword">then</span>
<a name="l00114"></a>00114       bcname = <span class="stringliteral">&#39;Special&#39;</span>
